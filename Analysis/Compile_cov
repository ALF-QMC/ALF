TARGET= cov_tau.out
OBJS=  cov_tau.o
TARGET2= cov_tau_sym.out
OBJS2=  cov_tau_sym.o

all: $(TARGET) $(TARGET2)
  
.SUFFIXES: .f90 .f
.f.o .f90.o:
	$(FC) -c -cpp -o $@ $(FLAGS) $<

$(TARGET): $(OBJS)
	$(FC) $(LF) -o $(TARGET) $(OBJS) $(LIBS)

$(TARGET2): $(OBJS2)
	$(FC) $(LF) -o $(TARGET2) $(OBJS2) $(LIBS)

clean:
	rm -f  $(OBJS)  $(OBJS2)

